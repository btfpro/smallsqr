!*****************************************************************
begin-procedure Check-for-Alpha-Chars($InputString, :$FormattedString)
!******************************************************************
!  This procedure will check to see if the value in $InputString is number or alphabetic.
!  If it is Alphabetic we will left justify it.  If it is numeric we will zero fill it if necerry.
!  This is going to be used for the "Step" variables and assumes that the output
! variable is for inserting in to a Char(2) field.
!

if isnull(rtrim($inputstring,' '))
        let $formattedString = '  '
end-if

! ASCII Characters 0-9 are represented by number 48 - 57
! so if the first character is in this range we can assume in is number and zero fill
! it.
 If (Ascii(rtrim($inputString,' ')) >= 48 AND ASCII(rtrim($InputString,' ')) <= 57)
        If Length(rtrim($inputstring,' ')) = 1
                let $formattedString = '0' || rtrim($inputstring, ' ')
        else
                let $formattedString = $inputString
        end-if
else
        If Length(ltrim(rtrim($inputstring,' '),' ')) = 1
                let $formattedString = ltrim(rtrim($inputString, ' '),' ') || ' '
        else
                let $formattedString = $inputString
        end-if
end-if
end-procedure


!***********************************************************************
begin-procedure Find-Dyn-Defaults
!***********************************************************************

begin-select DISTINCT
DEF.GVT_CPDF_FLD_NBR
FLD.GVT_CPDF_DEF_VALUE

  let #fld-number = &def.gvt_cpdf_fld_nbr
  let $def-value  = &fld.gvt_cpdf_def_value

  Do Default-Values

FROM PS_GVT_CPDF_ERRTBL ERR,
     PS_GVT_CPDF_FLDTBL FLD,
     PS_GVT_CPDF_DEF    DEF
WHERE ERR.EMPLID             = &dy.EMPLID
AND   ERR.EMPL_RCD          = &dy.EMPL_RCD
AND   ERR.GVT_CPDF_NOA_EFFDT = $NoaEffst
AND   ERR.GVT_CPDF_SEQ_NBR   = &dy.EFFSEQ
AND   ERR.GVT_EDIT_PREFIX    = DEF.GVT_EDIT_PREFIX
AND   ERR.GVT_EDIT_CATEGORY  = DEF.GVT_EDIT_CATEGORY
AND   ERR.GVT_EDIT_ID        = DEF.GVT_EDIT_ID
AND   ERR.GVT_EDIT_ID IN ('2', '3')
AND   DEF.GVT_CPDF_FLD_NBR   = FLD.GVT_CPDF_FLD_NBR
end-select

end-procedure


!***********************************************************************
begin-procedure Find-Stat-Defaults
!***********************************************************************

begin-select DISTINCT
DEF1.GVT_CPDF_FLD_NBR
FLD1.GVT_CPDF_DEF_VALUE


 let #fld-number = &def1.gvt_cpdf_fld_nbr
 let $def-value  = &fld1.gvt_cpdf_def_value

 Do Default-Values

FROM PS_GVT_CPDF_ERRTBL ERR1,
     PS_GVT_CPDF_FLDTBL FLD1,
     PS_GVT_CPDF_DEF    DEF1
WHERE ERR1.EMPLID              = &st.emplid
AND   ERR1.EMPL_RCD            = &ST.EMPL_RCD
AND   ERR1.GVT_EDIT_PREFIX     = DEF1.GVT_EDIT_PREFIX
AND   ERR1.GVT_EDIT_CATEGORY   = DEF1.GVT_EDIT_CATEGORY
AND   ERR1.GVT_EDIT_ID         = DEF1.GVT_EDIT_ID
AND   ERR1.GVT_EDIT_ID  IN ('1', '3')
AND   DEF1.GVT_CPDF_FLD_NBR    = FLD1.GVT_CPDF_FLD_NBR

end-select

end-procedure


!***********************************************************************
begin-procedure Default-Values
!***********************************************************************

