begin-procedure Ask-Dynamic-Status-Submission

  display ''
  display 'Do you wish to perform a CPDF status submission? (Y/N)'
  display ''

  input $status-run maxlen=1
  uppercase $status-run

  if $status-run <> 'Y'
    let $status-run = 'N'
  end-if

  display ''
  display 'Do you wish to perform a CPDF dynamics submission? (Y/N)'
  display ''

  input $dynamic-run maxlen=1
  uppercase $dynamic-run

  if $dynamic-run <> 'Y'
    let $dynamic-run = 'N'
  end-if

end-procedure

