Let $E_PHONE = &E.POSITION_PHONE   
!let $tmp = substr(&E.PHONE,1,3)  
let $tmp = substr($E_PHONE,1,3)
!End Resolution 574140
print $tmp                                      (62,97) BOLD
!Begin Resolution 574140
!let $tmp = substr(&E.PHONE,1,3)  
