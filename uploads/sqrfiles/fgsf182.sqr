Let $Grade = &J.GRADE               
Let $Step =  &J.STEP                 

!let $tmp = &J.GVT_PAY_PLAN ||'/'|| &JC.GVT_OCC_SERIES ||'/'|| &J.GRADE ||'/'|| edit(&J.STEP,'09')
