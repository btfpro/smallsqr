graphic () font 4 6
display &D.GVT_PAR_DESCR2
display &D.GVT_PAR_DESCR3
display &D.GVT_PAR_DESCR4
display &D.GVT_PAR_DESCR5

print &SUB.GVT_DESCR70                          (53, 4) BOLD
if &D.GVT_PAR_DESCR2 <> ' '
