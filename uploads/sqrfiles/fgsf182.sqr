   print &TRN.ADDRESS1_OTHER                    (82, 101) BOLD
   print &TRN.ADDRESS2_OTHER                    (+3, 101) BOLD
   if &TRN.CITY_OTHER <> ' '
       let $tmp = &TRN.CITY_OTHER ||', '||&TRN.STATE_OTHER||' '||&TRN.POSTAL_OTHER
   end-if
   print $tmp                                   (+3, 101) BOLD
end-if
