   if &TRN.CITY_OTHER <> ' '
       let $tmp = &TRN.CITY_OTHER ||', '||&TRN.STATE_OTHER||' '||&TRN.POSTAL_OTHER
   end-if
   print $tmp                                   (+3, 101) BOLD
end-if

print &TRN.COURSE_TITLE                         (93,6) BOLD
graphic () font 4 7

Move &TRN.GVT_TRNG_OBJS to $PrnText
move 6 to #PrnStartCol
move 95 to #PrnStartRow
