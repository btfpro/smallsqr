begin-procedure LongNameAddress

  next-column

  print $A.Descr (45,2)
  print $A.Address1 (+1,2)
  if RTRIM($A.Address2, ' ') <> ''
    print $A.Address2 (+1,2)
  end-if
  print $C4 (+1,2)

  if RTRIM($E.Address2, ' ') <> ''
    if $Use_Alternate_Name = 'Y'
        print $Alternate_Name     (60,21)
    else
        print $E.YE_Name          (60,21)
    end-if
    print $E.Address1    (+1,21)
    print $E.Address2    (+1,21)
    print $F3            (+1,21)
