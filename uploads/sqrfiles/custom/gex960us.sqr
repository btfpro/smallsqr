  move #FormsPerPage to #EmployeesPerPage
  move 1 to #k
  while #k <= #Align_Count
    do Load-Test-Data

    if $Form_ID = 'LPS4V' and (#k = 1 or mod(#k - 1,4) = 0)
       do LongNameAddressEE
    end-if

    do Print-W2
    if $Form_ID = 'LW2PS' and mod(#k,4) = 0   ! For the 4th form in every page.
      do LongNameAddress
    end-if
    add 1 to #k
