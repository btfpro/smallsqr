       FROM   PS_JOB 
       WHERE  EMPLID     = q.EMPLID
          AND EMPL_RCD   = q.EMPL_RCD
          and action     = q.action 
          AND EFFDT      = q.EFFDT)

AND NOT EXISTS
(SELECT 'X'	  
FROM  PS_JOB r
