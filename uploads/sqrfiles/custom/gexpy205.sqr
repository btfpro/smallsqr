!*********************************************************************
!This procedure determines the number of weeks allowed based on the 
!service years for the employee.
!*********************************************************************

begin-procedure Calc-Eligible-Weeks
#debug9 Show 'Calc-Eligible-Weeks'
  Let #Elig_Weeks_06 = 0	!GEX-TLL 05/14/2008 for 2006 schedule
  !AXL 3/20/2002 - New EVALUATE level for UNION_CD - Begin
  !PSR 08/01/2002
  !evaluate &JL.UNION_CD
   evaluate &A.UNION_CD
  !PSR 08/01/2002
    when = 'TOL'
         evaluate #dtu_years 
           when >= 8
             move 3 to #eligible_weeks
             break
           when >= 3
             move 2 to #eligible_weeks
             break
           when >= 1
             move 1 to #eligible_weeks
             break
         end-evaluate
!$#######################################################
! ITG 39788
! CWB 01/25/2007 BEGIN
    when-other 
      evaluate &a.deptid
        when = '4097'
          evaluate #dtu_years 
            when >= 20
              move 5 to #eligible_weeks
              break
            when >= 15
              move 4 to #eligible_weeks
              break
            when >= 10
              move 3 to #eligible_weeks
              break
            when >= 3
              move 2 to #eligible_weeks
              break
            when >= 1
              move 1 to #eligible_weeks
              break
          end-evaluate
!*********************************************************
! CWB New schedule for 2007
!*********************************************************
        when-other 
         do Get-2006-Elig-Weeks	!GEX-TLL 05/14/2008 Added for 2006 schedule
          evaluate #dtu_years 
           when >= 20
           !GEX-TLL 05/14/2008 Commented - Start
             !do get-curr-elig-weeks     
             !if #curr_elig_weeks > 5
              ! move #curr_elig_weeks to #eligible_weeks
             !else 
               move 5 to #eligible_weeks
             !end-if
             break
           when >= 15
             !do get-curr-elig-weeks     
             !if #curr_elig_weeks > 4
               !move #curr_elig_weeks to #eligible_weeks
             !else 
               move 4 to #eligible_weeks
             !end-if
             break
           when >= 10
             !do get-curr-elig-weeks     
             !if #curr_elig_weeks > 3
              ! move #curr_elig_weeks to #eligible_weeks
             !else 
               move 3 to #eligible_weeks
             !end-if 
             break
           when >= 3
             !do get-curr-elig-weeks     
             !if #curr_elig_weeks > 2
              ! move #curr_elig_weeks to #eligible_weeks
             !else 
               move 2 to #eligible_weeks
             !end-if 
             !GEX-TLL 05/14/2008 Commented - End 
             break
           when >= 1
             move 1 to #eligible_weeks
             break
         end-evaluate
       end-evaluate
    end-evaluate
!**********************************************************
! Old schedule, prior to 2007
!**********************************************************
!       when-other
!          evaluate #dtu_years 
!            when >= 23
!              move 6 to #eligible_weeks
!              break
!            when >= 17
!              move 5 to #eligible_weeks
!              break
!            when >= 12
!              move 4 to #eligible_weeks
!              break
!            when >= 7
!              move 3 to #eligible_weeks
!              break
!            when >= 3
!              move 2 to #eligible_weeks
!              break
!            when >= 1
!              move 1 to #eligible_weeks
!              break
!          end-evaluate
!      end-evaluate
!   end-evaluate
! CWB 01/25/2007 END
!$#######################################################
    !AXL 3/20/2002 - New EVALUATE level for UNION_CD - End
!GEX-TLL 05/14/2008 Begin
If #Elig_Weeks_06 > #eligible_weeks
      !show '#Elig_Weeks_06 > #eligible_weeks'	!vendsxn Testing
      Let #eligible_weeks = #Elig_Weeks_06
   End-If
!GEX-TLL 05/14/2008 End
end-procedure Calc-Eligible-Weeks
!***********************************************************************************
!GEX-TLL 05/14/2008 Added this Procedure for 2006 Schedule - Start
