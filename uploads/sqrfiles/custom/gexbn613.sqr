!********************************************
Begin-Procedure P100-Initialization
!********************************************
#debug9 Show 'P100-Initialization'
  Do Init-DateTime           !datetime.sqc
  Do Init-Number             !number.sqc
  Do Get-Current-DateTime    !curdttim.sqc
  Do Stdapi-Init             !stdapi.sqc

  Let $ReportID      =   'GEXBN613'
  Let $ReportTitle   =   'Marsh Voluntary Benefits Deductions'

  Show '$ReportID    = ' $ReportID
  Show '$ReportTitle = ' $ReportTitle

  Let $GEXXX919_Dedcd_Alias = 'DED.DEDCD'	!AXL 03/08/2006

  If $prcs_process_instance = ''
    Do Ask-As-Of-Date
    
    !AXL 03/08/2006 - Begin
    Let $DedcdString = ''''
    Display 'Enter Dedcd or leave blank to exit.'
    While 1=1
      Input $Dedcd Maxlen=6 'Dedcd'
      Uppercase $Dedcd
      If Rtrim($Dedcd, ' ') = ''
        Concat '''' With $DedcdString
        Break
      End-If
      If $DedcdString <> ''''
        Concat ''',''' With $DedcdString
      End-If
      Concat $Dedcd With $DedcdString
    End-While

    If $DedcdString = ''''''
      Let $GEXXX919_INCLUDE_DEDCD_CRITERIA = '1=1'
      Let $GEXXX919_INCLUDE_DEDCD_DISPLAY_CRITERIA = 'ALL'
    Else
      Let $GEXXX919_INCLUDE_DEDCD_CRITERIA = $GEXXX919_Dedcd_Alias || ' In (' || $DedcdString || ')'
      Let $GEXXX919_INCLUDE_DEDCD_DISPLAY_CRITERIA = $GEXXX919_INCLUDE_DEDCD_CRITERIA
    End-If
    !AXL 03/08/2006 - End

  Else
    Do GEXRCBN1-SELECT-PARAMETERS
    Let $AsOfDate = &GEX_RC_BEN.ASOFDATE
    If Rtrim($AsOfDate, ' ') = ''
      Let $AsOfDate = $AsOfToday
    End-If

    Do GEXXX919-Select-DEDCD-Parameters		!AXL 03/08/2006

  End-If

  let $effdt = $AsOfDate

  date-time () HH:MI:SS &timeBegan
  date-time () MM/DD/YYYY &dateBegan
  show 'Report Began at ' &timeBegan ' on ' &dateBegan
  show ' '

  let $Data-Found-Flag = 'N'

  Do P140-Open-File
End-Procedure


!********************************************
Begin-Procedure P140-Open-File
!********************************************
#debug9 Show 'P140-Open-File'

!Open Input file
!***************
  Let $file1 = '{INFILE}' || Lower($ReportID) || '.dat'
!  Let $file1 = 't:\psoft\hr751\working\isdvgbd\' || Lower($ReportID) || '.dat'

  Show $file1

  Open $file1 as 1
       For-Reading Record=69 Status=#filestat    

  If #filestat != 0
     Show 'Error opening input file.  Program terminating.'
     Stop
  End-If

End-Procedure


!********************************************
Begin-Procedure P200-Main-Process
!********************************************
#debug9 Show 'P200-Main-Process'

  let #recs-read  = 0
  let #trans-read = 0
  let #recs-error = 0
  let #recs-inserted = 0
  let #recs-termed   = 0
  let #recs-updated  = 0
  let #recs-zero  = 0

  While 1=1

     Read 1 Into $ssn:9 !$emplid:9 !GEX_SEC_REENG 2015-04-29 Vahini Katta
	               $lastname:20
                 $firstname:15
	               $marsh_dedcd:6
   	             $amount:10
	               $status:1
	               $marsh_effdtin:8

          Status=#filestat

    If #filestat != 0
       Show 'Error reading input file.  Program terminating.'
       Stop
    End-If

    If #end-file 
      Break
    Else
      Add 1 To #recs-read
      Add 1 To #trans-read
    End-If

    Use-Report Log-Report

!    If $message = 'No transactions to process'
!      
!      show $message
!      print $message (+1,1)
!
!    Else

      let $error_row = $emplid||$lastname||$firstname||$marsh_dedcd||$amount
                       ||$status||$marsh_effdtin

      let $name = rtrim($lastname,' ')||','||rtrim($firstname,' ')

      show 'Read Record'
      show 'ssn:     ' $ssn
      show 'Name:    ' $name
      show 'Dedcd:   ' $marsh_dedcd
      show 'Amount:  ' $amount
      show 'Status:  ' $status
      show 'Effdt:   ' $marsh_effdtin
      
      do Get-Emplid !GEX_SEC_REENG 2015-04-29 Vahini Katta
      
      Let #Amount = $amount
!      Let #Amount2 = $amount2

      !Let #Amount = #Amount/100
!      Let #Amount2 = #Amount2/100

!      show 'Marsh Effdt: ' $marsh_effdtin
      let $marsh_effdtin1 = strtodate($marsh_effdtin,'YYYYMMDD')  !AXL 5/27/2005 Add
!     show '$marsh_effdtin1 ' $marsh_effdtin1
      let $marsh_yy = substr($marsh_effdtin,1,4)
      let $marsh_mm = substr($marsh_effdtin,5,2)
      let $marsh_dd = substr($marsh_effdtin,7,2)
      let $marsh_effdt = $marsh_mm||'/'||$marsh_dd||'/'||$marsh_yy
!      show 'Marsh Effdt: ' $marsh_effdt

!**
      let $Year4=1
!      show 'Effdt: ' $effdt
      Do Format-DateTime($effdt,$comp_effdt,{DEFDMY},'','')
!      show '$comp_effdt: ' $comp_effdt
      let $comp_dd = substr($comp_effdt,1,2)
      let $comp_mm = substr($comp_effdt,4,2)
      let $comp_yy = substr($comp_effdt,7,2)
      Do MakeYear4Digits($comp_yy)
      let $comp_effdt = $comp_mm||$comp_dd||$comp_yy
!      show 'Comp effdt: ' $comp_effdt
      
      let #marsh_mm = $marsh_mm
      let #comp_mm = $comp_mm
      let #marsh_yy = $marsh_yy
      let #comp_yy = $comp_yy
      let $continue = 'Y'

      if (#marsh_mm = #comp_mm) or (#marsh_mm = #comp_mm + 1) or (#marsh_mm = #comp_mm + 2)
         let $continue = 'Y'
      else
         if #marsh_mm > #comp_mm + 2
            let $continue = 'N'
            Add 1 to #recs-error         
            show $emplid ' has a future dated effective date. No rows inserted.'
            Print $emplid                                                 (+1,{#column1})
            Print 'has a future dated effective date. No rows inserted.'  (+0,{#column2})
            Print $error_row                                              (+1,{#column2})
         end-if
      end-if
!**

      !AXL 03/08/2006 - Check DEDCD against run control - Begin
      let $dedcd_valid = 'N'
      Do Validate-Dedcd
      If $dedcd_valid = 'N'
         let $continue = 'N'
            Add 1 to #recs-error         
            show $emplid ' has a DEDCD of ' $marsh_dedcd '. Code is not on Run Control panel. No rows inserted.'
            Print $emplid                                                 (+1,{#column1})
            Print 'has a bad dedcd. No rows inserted.'  (+0,{#column2})
            Print $error_row                                              (+1,{#column2})
      end-if
      !AXL 03/08/2006 - Check DEDCD against run control - End

      If $continue = 'Y'

         let $job_exist = 'N'
         Do Check-Job
   
!         Show '$job_exist:     ' $job_exist
!         Show '$no_job_active: ' $no_job_active
!         Show '$multi_job:     ' $multi_job

         if $job_exist = 'Y'
            if $no_job_active = 'N'
               if $status = 'N' and #Amount = 0
                  Use-Report Log-Report   
                  show $emplid ' is new and has no amounts for dedcd: ' $marsh_dedcd '. No row inserted.'
                  Print $emplid                                                 (+1,{#column1})
                  let $out = 'Emplid is new and has no amounts for dedcd: ' || $marsh_dedcd ||'. No rows inserted.'
                  print $out                                                    (+0,{#column2})
                  Print $error_row    (+1,{#column2})
                  Add 1 to #recs-zero
                  Add 1 to #recs-error   !1 rows rejected 
               Else
!                  Let $marsh_dedcd = $marsh1_dedcd
                  Let #Ded_Amt     = #Amount
                  Add #Ded_Amt    to #MARSH_TOT
                  
                  Do Process-Load
                  
!                  Let $marsh_dedcd = $marsh2_dedcd
!                  Let #Ded_Amt     = #Amount2
!                  Add #Ded_Amt    to #MARSH2_TOT
                  
!                  Do Process-Load      
               End-if
            else
               Use-Report Log-Report   
               show $emplid ' does not have an active Corporate, Non Union Job row. Did not process.'
               Print $emplid                                                                          (+1,{#column1})
               Print 'Emplid does not have an active Corporate, Non Union Job row. Did not process.'  (+0,{#column2})
               Print $error_row                                                                       (+1,{#column2})
               Add 2 to #recs-error   !2 rows rejected - MARSH1&2
            end-if
         else
            Use-Report Log-Report
            if $multi_job = 'N'
               show $emplid ' does not exist on Job. No row inserted'
               Print $emplid                                                 (+1,{#column1})
               Print 'does not exist on Job table. No row inserted.'         (+0,{#column2})
            Else
               show $skip_message
               Print $skip_message                                           (+1,{#column1})
            End-if
            Print $error_row                                                 (+1,{#column2})
            Add 2 to #recs-error
         End-If
      End-If

!    End-if

    skip-record:

  End-While

  Use-Report Load-Report
  Do Print-Load-Totals

End-Procedure


!********************************************
Begin-Procedure Check-Job
!********************************************
#debug9 Show 'Check-Job'

  Let $no_job_active = 'N'
  Let $multi_job = 'N'
  Let #active_count = 0
  Let $first_check = 'Y'
  Let $skip_message = ' '

begin-select
CJ.EMPLID
CJ.EMPL_RCD    &cj.empl_rcd
CJ.COMPANY     &cj.company
CJ.PAYGROUP    &cj.paygroup
CJ.EMPL_STATUS &cj.empl_status

   let $job_exist = 'Y'

   let #check_empl_rcd    = &cj.empl_rcd
   let $check_empl_status = &cj.empl_status

   if $check_empl_status = 'A' or $check_empl_status = 'L' or
      $check_empl_status = 'P' or $check_empl_status = 'S'

      Add 1 to #active_count
      
      if $first_check = 'Y'
         let $hold_company = &cj.company
         let $hold_paygroup = &cj.paygroup
         let $first_check = 'N'
      end-if


      if #check_empl_rcd > 0
         if #active_count > 1
            let $job_exist = 'N'
            let $multi_job = 'Y'
            let $skip_message = $emplid||'    is an active multi job employee. No row inserted.'
            show $skip_message
         else
            let $no_job_active = 'N'
            let #cj_empl_rcd = &cj.empl_rcd
            let $cj_company  = &CJ.COMPANY
            let $cj_paygroup = &CJ.PAYGROUP
            let $cj_empl_status = &CJ.EMPL_STATUS
         end-if
      else
            let $no_job_active = 'N'
            let #cj_empl_rcd = &cj.empl_rcd
            let $cj_company  = &CJ.COMPANY
            let $cj_paygroup = &CJ.PAYGROUP
            let $cj_empl_status = &CJ.EMPL_STATUS
            
            if $hold_company <> $cj_company
               let $job_exist = 'N'
               let $multi_job = 'Y'
               let $skip_message = $emplid||'    has two active Job rows: '||$hold_company||' and '||$cj_company||'.'
            else
               if $hold_paygroup <> $cj_paygroup
                  let $job_exist = 'N'
                  let $multi_job = 'Y'
                  let $skip_message = $emplid||'    has two active Job rows: '||$hold_company||'/'||$hold_paygroup||' and '||$cj_company||'/'||$cj_paygroup||'.'
               end-if
            end-if
      end-if
   else
      if #active_count = 0
         let $no_job_active = 'Y'
      end-if
   end-if

FROM PS_JOB CJ
!, PS_JOB_LABOR JL - AXL 03/08/2006 - Join to JOB_LABOR no longer needed
WHERE CJ.EMPLID    = $emplid
!AND   CJ.EMPLID    = JL.EMPLID
!AND   CJ.EMPL_RCD = JL.EMPL_RCD
AND   CJ.EFFDT = (SELECT MAX(EFFDT)
      FROM PS_JOB
      WHERE EMPLID    = CJ.EMPLID
      AND   EMPL_RCD = CJ.EMPL_RCD
      AND   TO_CHAR(EFFDT,'YYYYMMDD') <= $marsh_effdtin)
AND   CJ.EFFSEQ = (SELECT MAX(EFFSEQ)
      FROM PS_JOB
      WHERE EMPLID    = CJ.EMPLID
      AND   EMPL_RCD = CJ.EMPL_RCD
      AND   EFFDT     = CJ.EFFDT)
!AND   JL.EFFDT = (SELECT MAX(EFFDT)
!      FROM PS_JOB_LABOR
!      WHERE EMPLID    = JL.EMPLID
!      AND   EMPL_RCD = JL.EMPL_RCD
!     AND   TO_CHAR(EFFDT,'YYYYMMDD') <= $marsh_effdtin)
!AND   JL.EFFSEQ = (SELECT MAX(EFFSEQ)
!     FROM PS_JOB_LABOR
!      WHERE EMPLID    = JL.EMPLID
!      AND   EMPL_RCD = JL.EMPL_RCD
!      AND   EFFDT     = JL.EFFDT)
!AND   CJ.COMPANY IN ('BRM','GEI','KAP','RFI','RTP','TMK')  !Corporate
!AND   CJ.UNION_CD IN ('NON','COL','TOL','HGN','MOD','W05')       !Non Union
ORDER BY CJ.EMPLID, CJ.EMPL_RCD
end-select

End-Procedure

!AXL 03/08/2006 - New Validation - Begin
!********************************************
Begin-Procedure  Validate-Dedcd
!********************************************
#debug9 Show ' Validate-Dedcd'

begin-select
'X'

   Let $dedcd_valid  = 'Y'

FROM PS_DEDUCTION_TBL DED
  WHERE DED.EFFDT =
        (SELECT MAX(EFFDT) FROM PS_DEDUCTION_TBL
        WHERE DED.PLAN_TYPE = PLAN_TYPE
          AND DED.DEDCD     = DEDCD
          AND EFFDT <= SYSDATE)
  AND [$GEXXX919_INCLUDE_DEDCD_CRITERIA]
  AND DED.DEDCD = $marsh_dedcd

end-select
End-Procedure
!AXL 03/08/2006 - End

