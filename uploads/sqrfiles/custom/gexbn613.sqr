Begin-Procedure Process-Load
!*****************************************
#debug9 Show 'Process-Load'

   Do Get-Pay-Begin-Dt-A

   if $pc_pay_end_dt = ' '

      Show $Emplid ' could not find the Pay End Dt for Company/Paygroup ' $cj_company ' ' $cj_paygroup '. No data inserted.'
      Print $emplid                                                                (+1,{#column1})
      Print 'could not find the Pay End Dt for company/paygroup '                  (+0,{#column2})
      Print $cj_company                   (+0,0)
      Print ' '                           (+0,0)
      Print $cj_paygroup                  (+0,0)
      Print '. No data inserted.'         (+0,0)
      Print $error_row                                                             (+1,{#column2})
      Add 1 to #recs-error

   else
      let $genl_ded_exist = 'N'
      Do Check-Genl-Deduction
      If $genl_ded_exist = 'N'
   
         If #Ded_Amt > 0
            Do Insert-Genl-Deduction
         Else
            Use-Report Log-Report
!            if $status = 'N'
!               Show $Emplid ' has a new ' $marsh_dedcd ' deduction with no amount. No data inserted.'
!               Print $emplid                       (+1,{#column1})
!               Print 'has a new '                  (+0,{#column2})
!               Print $marsh_dedcd                  (+0,0)
!               Print ' deduction with no amount.'  (+0,0)
!               Print ' No data inserted.'          (+0,0)
!            else
!               Show $Emplid ' has a change/delete ' $marsh_dedcd ' deduction with no amount. No data inserted.'
!               Print $emplid                       (+1,{#column1})
!               Print 'has a change/delete '        (+0,{#column2})
!               Print $marsh_dedcd                  (+0,0)
!               Print ' deduction with no amount.'  (+0,0)
!               Print ' No data inserted.'          (+0,0)
!            end-if
!            Add 1 to #recs-zero
         End-if

      Else
         If $status = 'C'
            If #Ded_Amt = 0
!               Show 'Term'
               Do Term-Genl-Deduction
            else
!               Show 'Update'
               Do Check-Genl-Deduction2
            end-if
         else
!            Show 'Update2'
            Do Check-Genl-Deduction2
         end-if

      End-if   

   End-if

End-Procedure


!********************************************
Begin-Procedure Get-Pay-Begin-Dt-A
!********************************************
#debug9 Show 'Get-Pay-Begin-Dt'

  Let $pc_pay_end_dt = ' '

Begin-Select Loops=1
pc.pay_end_dt     &pc.pay_end_dt
pc.check_dt
pc.pay_begin_dt   &pc.pay_begin_dt
pc.pay_begin_dt-1 &pc.prior_pay_end_dt
pc.pay_period

To_Char(pc.pay_begin_dt,'YYYYMMDD') &pc.pay_begin_date
  Let $pay_begin_dt     = &pc.pay_begin_dt
  show '$pay_begin_dt from Get-Pay-Begin-Dt-A '$pay_begin_dt	!AXL 5/27/2005 Add
  Let $pay_begin_date   = &pc.pay_begin_date
  Let $prior_pay_end_dt = &pc.prior_pay_end_dt

  Let $pc_pay_end_dt   = &pc.pay_end_dt
  Let $pc_pay_begin_dt = &pc.pay_begin_dt

!AXL 5/27/2005 Disable old and enable newer logic - Begin
!From  ps_pay_calendar pc
!Where pc.company    = $cj_company
!And   pc.paygroup   = $cj_paygroup
!And   pc.pay_end_dt = (select max(pay_end_dt)
!      from ps_pay_calendar
!       where company = pc.company
!     and   paygroup = pc.paygroup
!      and   to_char(pay_end_dt,'YYYYMMDD') <= $marsh_effdtin) 
!And   pc.pay_period = 1
!And   pc.run_id not like '%YE'
!
From ps_pay_calendar pc
Where pc.company    = $cj_company
And   pc.paygroup   = $cj_paygroup
And   To_Char(pc.check_dt,'YYYYMM')
                  = To_Char(Add_Months($marsh_effdtin1,+1),'YYYYMM')
And   pc.pay_period = 1
And   pc.run_id not like '%YE'
!AXL 5/27/2005 Disable old and enable newer logic - End

End-Select

 if $pc_pay_end_dt = ' '

   Do Get-Pay-Begin-Dt-B

 end-if

End-Procedure


!********************************************
Begin-Procedure Get-Pay-Begin-Dt-B
!********************************************
#debug9 Show 'Get-Pay-Begin-Dt-B'

  Let $marshb_mm = substr($marsh_effdt,1,2)
  Let $marshb_dd = substr($marsh_effdt,4,2)
  Let $marshb_yy = substr($marsh_effdt,7,4)
  Let $marshb_dt = $marsh_yy||'-'||$marsh_mm||'-'||$marsh_dd
!  show '$marshb_dt: ' $marshb_dt

  do dtu-add-days($marshb_dt, +7, $marshb_effdt)
!  show '$marshb_dt+7: ' $marshb_dt

  Let $marshb_yy = substr($marshb_effdt,1,4)
  Let $marshb_mm = substr($marshb_effdt,6,2)
  Let $marshb_dd = substr($marshb_effdt,9,2)
  Let $marsh_effdt_b = $marshb_yy||$marshb_mm||$marshb_dd
!  show 'Marsh_Effdt_b: ' $marsh_effdt_b
  let $marsh_effdt_b2 = strtodate($marsh_effdt_b,'YYYYMMDD')  !AXL 5/27/2005 - add
