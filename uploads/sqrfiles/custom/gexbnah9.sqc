   print $bp_name ()
   print ' in GEX_CONFG2_RULE table' ()
 
else
!SXK 06/15/98 No need to report, if they don't meet the eligibility'
  if $byte_found = 'N'
      move 'Y' to $errorfound
!     do error-found
!     print 'Eligible Config byte 2 rules not satisfied. #HOURS = ' (,34) 
!     print #week_hrs ()

 end-if
end-if

