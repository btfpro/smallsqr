    else 
	if $rules_found = 'Y' 
	   do prepare-comparision-string 
	   do get-ec2-byte-value
	end-if
   end-if  

      
FROM PS_GEX_CONFG2_PGM G, PS_GEX_CONFG2_RULE F,

!***SXK  04/07/1998 Changed to retrieve right row
     PS_BEN_DEFN_PGM FF
WHERE G.BENEFIT_PROGRAM = $bp_name
