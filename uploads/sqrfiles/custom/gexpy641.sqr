        AND P205DEDBAL1.BALANCE_PERIOD <= &WT.BALANCE_PERIOD) 

  AND P205DEDBAL.PLAN_TYPE     = '00'   
  !10/18/2000 - AXL - Begin                      
  !AND P205DEDBAL.DEDCD   LIKE    '4%'            
  AND (P205DEDBAL.DEDCD LIKE   '4%' or P205DEDBAL.DEDCD LIKE   'LOAN0%')
  !10/18/2000 - AXL - End
             
end-SELECT 

