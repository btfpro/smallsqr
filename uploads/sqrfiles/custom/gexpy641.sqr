 #DEBUG7 show 'P205DEDBAL.BENEFIT_PLAN   =' &P205DEDBAL.BENEFIT_PLAN     
 #DEBUG7 show 'P205DEDBAL.DEDCD          =' &P205DEDBAL.DEDCD     
