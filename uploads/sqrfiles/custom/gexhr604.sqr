BEGIN-SELECT 
!COUNT(DISTINCT HD.DEPENDENT_BENEF) &HD.DEPENDENT_COUNT                   !DS 03/28/03
COUNT(HD.DEPENDENT_BENEF) &HD.DEPENDENT_COUNT                             !DS 03/28/03
