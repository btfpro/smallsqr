!************************************************************************
BEGIN-PROCEDURE FIND-OTHER-INFO
!************************************************************************
BEGIN-Select Loops=1

OTH.EMPLID
OTH.EMPL_RCD
OTH.COMPANY
OTH2.DESCR
  
  let $disclaimer_one = '0001'
  !    'This employee is active in multiple jobs within the Giant Ea'
  !let $disclaimer_two =
  !    'gle entities.  Income verification information includes tota'
  !let $disclaimer_three =
  !    'l wages received from all jobs.  Employment verification dat'
  !let $disclaimer_four =
  !   'a reflects the job with the greatest service.'
 
from PS_JOB OTH, 
     PS_JOBCODE_TBL OTH2
  WHERE OTH.EFFDT =
        (SELECT MAX(EFFDT) FROM PS_JOB
        WHERE OTH.EMPLID = EMPLID
          AND OTH.EMPL_RCD = EMPL_RCD
          AND EFFDT <= $TALX_AS_OF_DATE_NATIVE)
    AND OTH.EFFSEQ =
        (SELECT MAX(EFFSEQ) FROM PS_JOB
        WHERE OTH.EMPLID = EMPLID
          AND OTH.EMPL_RCD = EMPL_RCD
          AND OTH.EFFDT = EFFDT)
    AND OTH2.EFFDT =
        (SELECT MAX(EFFDT) FROM PS_JOBCODE_TBL
        WHERE OTH2.SETID =   SETID
          AND OTH2.JOBCODE = JOBCODE
          AND EFFDT <= OTH.EFFDT)
    AND OTH.jobcode = OTH2.jobcode
    AND OTH.EMPLID   = $SAVE_EMPLID
    AND OTH.EMPL_RCD <> #SAVE_EMPL_RCD

END-SELECT

