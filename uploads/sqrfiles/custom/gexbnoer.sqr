	   if &K.BENEFIT_PLAN = 'PGE01B' !or &K.BENEFIT_PLAN = 'XGE01D'
	      let $table_name = 'PS_HEALTH_BENEFIT CE'
	      let $table_name_1 = 'PS_HEALTH_BENEFIT OOO1'
	      let $table_name_2 = 'PS_HEALTH_BENEFIT OO1'
	      do check-current-election
	      if $enrolled = 'N'
		 let $print_cost = 'N'
