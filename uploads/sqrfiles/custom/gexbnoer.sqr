Begin-Procedure Select-Price-Amt
!*******************************

let #TmpAmt = 0

Begin-SELECT

N.SCHED_ID
N.EMPLID
N.BENEFIT_RCD_NBR
N.EVENT_ID
N.PLAN_TYPE
N.OPTION_ID

! GEX ISDVNPK 08/28/2007 Modified for 9.0 upgrade
!SUM(N.ANNUAL_AMT) &n_annual_amt
!SUM(N.DEDUCT_AMT) &n_deduct_amt
SUM(N.ANNL_AMT_B_TAX)+SUM(N.ANNL_AMT_A_TAX) &n_annual_amt
SUM(N.DEDN_AMT_B_TAX)+SUM(N.DEDN_AMT_A_TAX) &n_deduct_amt
