let #col                 = 0
let #Optn_Cnt            = 0
let $first_sel = 'N'
        
Begin-SELECT
J1.SCHED_ID
J1.EMPLID
J1.BENEFIT_RCD_NBR
J1.EVENT_ID
J1.PLAN_TYPE     () On-Break Print=Never Before=On-Plan-Break
J1.EVENT_DT
J1.ELECT_ALLOWED
J1.BENEFIT_PROGRAM
J1.CUR_ELECT_IND
