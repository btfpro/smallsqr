J.EVENT_ID
J.PLAN_TYPE     () On-Break Print=Never Before=On-Plan-Break
J.EVENT_DT
J.ELECT_ALLOWED
J.BENEFIT_PROGRAM
J.CUR_ELECT_IND
K.SCHED_ID
K.EMPLID
K.BENEFIT_RCD_NBR
K.EVENT_ID
K.PLAN_TYPE
K.BENEFIT_PROGRAM
K.EVENT_DT
K.OPTION_ID
K.DISPLAY_PLN_SEQ
K.DISPLAY_OPT_SEQ
K.OPTION_TYPE
K.BENEFIT_PLAN
K.COVRG_CD
K.OPTION_CD
K.DEFAULT_IND
K.PROOF_REQ_IND
K.CALCULATED_BASE

  let $Plan_Type           = &J.Plan_Type
  let $CurElectFlag = &J.Cur_Elect_Ind
  let $ElectAllowFlag    = &J.Elect_Allowed
  let $print_cost = 'Y'
  let $Plan_Category = SUBSTR(&J.Plan_Type,1,1)
  let $benefit_program = &K.BENEFIT_PROGRAM		!vendtkk

  !CXA 11/3/2004 Begin
  let $Plan_Category_Flex = ' '
  if $Plan_Category = '6'
    let $Plan_Category_Flex = SUBSTR(&J.Plan_Type,2,1)
  else
    let $Plan_Category_Flex = ' '
  end-if
  !CXA 11/3/2004 End
 show '$benefit_program  : ' $benefit_program	
 if  $benefit_program = 'C01' or $benefit_program = 'C02' or $benefit_program = 'C03' or $benefit_program = 'C08'      !vendtkk
             ! let $ElectAllowFlag = 'N'  vendtkk 
             do Print-Benefit-Plan-Optns-2
 end-if

  if ($ElectAllowFlag = 'Y')
   
    !CXA 11/3/2004 Begin
    Evaluate $Plan_Category_Flex 
      when = '0'
        let $FSA_BLURB_2 = '* You must re-enroll each year to participate in the '
        let $FSA_BLURB_2 = $FSA_BLURB_2 || 'Health Care Spending Account. '
      when = '1'
        let $FSA_BLURB_2 = '* You must re-enroll each year to participate in the '
        let $FSA_BLURB_2 = $FSA_BLURB_2 || 'Dependent Care Spending Account. '
    End-Evaluate
    !CXA 11/3/2004 End
    if  $benefit_program <> 'C01' and $benefit_program <> 'C02' and $benefit_program <> 'C03' and $benefit_program <> 'C08'      !vendtkk
    	do Print-Benefit-Plan-Optns
    			                    
    end-if
    
    

    Evaluate $Plan_Category
      When = '1'
      When = '2'
      When = '3'
   !SXK 12/04/1998 Begin
   !    display $print_cost 
       if $plan_type <> '30' and $plan_type <> '31'
	  if ($plan_type = '21' and $print_cost = 'N') or
	     ($plan_type = '1Z' and $print_cost = 'N') 
	  else
	   do Select-Price-Amt
	   if  $benefit_program <> 'C01' and $benefit_program <> 'C02' and $benefit_program <> 'C03' and $benefit_program <> 'C08'      !vendtkk !Added GEX-CXB  10/16/2007  HCM 9.0 Upgrade
	   	do Print-Price-Amt
	   end-if
	  end-if
       end-if
   !SXK 12/04/1998 End
      When = '4'
	let #Cola = #CCol9 + 7
	 if  $benefit_program <> 'C01' and $benefit_program <> 'C02' and $benefit_program <> 'C03' and $benefit_program <> 'C08'      !vendtkk !Added GEX-CXB  10/16/2007  HCM 9.0 Upgrade
		do Print-Option-Cd
	  end-if
	let $IsEligForSvgsFlag = 'Y'
      When = '5'
      When = '6'
      When = '7'
      When = '8'
      When = '9'
	let #Cola = #CCol2
	 if  $benefit_program <> 'C01' and $benefit_program <> 'C02' and $benefit_program <> 'C03' and $benefit_program <> 'C08'      !vendtkk
		do Print-Option-Cd
	 end-if		!Added GEX-CXB  10/16/2007  HCM 9.0 Upgrade
    End-Evaluate

  end-if

FROM  PS_BAS_PARTIC_PLAN J,
      PS_BAS_PARTIC_OPTN K
WHERE J.SCHED_ID        = &A.Sched_Id
AND   J.EMPLID          = &A.EmplId
AND   J.BENEFIT_RCD_NBR    = &A.BENEFIT_RCD_NBR
AND   J.EVENT_ID        = &A.Event_Id
AND   J.PLAN_TYPE      <> '01'
!SXK 09/23/1999 Begin
AND   [$GEXXX912_INCLUDE_BENEFIT_PLAN_CRITERIA_1]
!SXK 09/23/1999 End
AND   K.SCHED_ID        = J.SCHED_ID
AND   K.EMPLID          = J.EMPLID
AND   K.BENEFIT_RCD_NBR    = J.BENEFIT_RCD_NBR
AND   K.EVENT_ID        = J.EVENT_ID
AND   K.PLAN_TYPE       = J.PLAN_TYPE
AND   K.OPTION_TYPE    IN ('O','W')
AND   K.HISTORY_ONLY    = 'N'
 !SXK 02/10/1999 Begin
AND   1 < (select count(plan_type) from ps_bas_partic_optn
 	           where     SCHED_ID        = K.SCHED_ID
 		AND   EMPLID          = K.EMPLID
 	       	AND   BENEFIT_RCD_NBR    = K.BENEFIT_RCD_NBR
 	      	AND   EVENT_ID        = K.EVENT_ID
 	       	AND   PLAN_TYPE       = K.PLAN_TYPE
 		AND   OPTION_TYPE    IN ('O','W')
 		AND   HISTORY_ONLY    = 'N')
!SXK 02/10/1999 End  
ORDER BY K.DISPLAY_PLN_SEQ ASC,
	  K.DISPLAY_OPT_SEQ ASC
End-SELECT
   
  do On-Plan-Break
  !AXL 9/22/2003 New blurb appearing before dependent section - Begin
  if $Plan_Category ='6'
     let $FSA_BLURB = '* If you are participating in the Health and/or Dependent Care '
     let $FSA_BLURB = $FSA_BLURB || 'Plans, you must re-enroll for '
     let $FSA_BLURB = $FSA_BLURB || $enroll_yyyy_plus_one || '.'
     print $FSA_BLURB  (+2,1) BOLD WRAP 120 6
     Graphic (+1,1,{NORM_LNLENGTH}) Horz-Line {NORM_LNSIZE} !AXL 9/22/2003 New dividing line
  end-if
  !AXL 9/22/2003 New blurb appearing before dependent section - End
  print '' (+1,1)
     
End-Procedure
     
!******************************
Begin-Procedure Select-Plan-1
!******************************
     
