  print '-'                   ( ,{#column4},14)  fill
  print '-'                   ( ,{#column5},14)  fill	! DXS, 02/20/07, Added 
  print '-'                   ( ,{#column6},30)  fill	! DXS, changed col number
  print '-'                   ( ,{#column7},4)   fill	! DXS, changed col number
  print '-'                   ( ,{#column8},5)   fill	! DXS, changed col number

