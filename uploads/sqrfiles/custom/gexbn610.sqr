#define #column4    42
#define #column5    60
#define #column6    78
#define #column7   102
#define #column8   111
#define #column9   125
#define #column10   135	! DXS, 02/20/07, added this.


Begin-Heading 8
#debug9 Show 'Begin-Heading'
  #Include 'stdhdg01.sqc'

  print 'PLAN'                (6,{#column1})
  print '  COMPENSATION'      ( ,{#column3})
  print '  COMPENSATION'      ( ,{#column4})
