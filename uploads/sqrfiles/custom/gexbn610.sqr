Begin-Procedure P250-Check-For-Enrollment
!*********************************************************************
#debug9 Show 'P250-Check-For-Enrollment'
  Let $enrolled = 'N'
  let $exclude_flag=' '
Begin-Select
SP.BENEFIT_PLAN
SP.EFFDT		&effdt
   Do format-datetime (&effdt,$term_dt_cmp,{DEFCMP},'','')   
SP.COVERAGE_ELECT
SP.COVERAGE_BEGIN_DT	&Covg_Beg_Dt
   Do format-datetime (&Covg_Beg_Dt,$Covg_Beg_Dt_cmp,{DEFCMP},'','')   	
SP.COVERAGE_ELECT_DT	&Covg_Elec_Dt
   Do format-datetime (&Covg_Elec_Dt,$Covg_Elec_Dt_cmp,{DEFCMP},'','')   	

  Let $benefit_plan = &SP.BENEFIT_PLAN
  
  #ifdef debug8
    display '&SP.COVERAGE_ELECT ' noline
    display &SP.COVERAGE_ELECT
    show '$benefit_plan ' $benefit_plan
  #endif

  Do format-datetime($year_end_dt,$year_end_dt_cmp,{DEFCMP},'','')   
  
  If &SP.COVERAGE_ELECT = 'E'
    Do P270-Check-Dreyfus-Plan
    #debug9 show 'Doing Get-New-Erncd'
    do Get-New-Erncd			! DXS, 03/01/07
  Else
    #debug9 show 'Doing P260-Get-Elect-Row'
    Do P260-Get-Elect-Row
    #debug9 show 'Doing Get-New-Erncd'
    do Get-New-Erncd			! DXS, 03/01/07
    Do P270-Check-Dreyfus-Plan
    !Do Convert-To-DTU-Date(&SP.EFFDT, $Term-Date)	AXL  doesn't work
    If $term_dt_cmp < $year_begin_dt_cmp
      Let $enrolled = 'N'
    End-If
  End-If
  
  #ifdef debug8
    show 'emplid in P250-Check-For-Enrollment proc ' $emplid
    display '&Covg_Beg_Dt ' noline
    display &Covg_Beg_Dt

    display '&Covg_Elec_Dt ' noline
    display &Covg_Elec_Dt

    display '$year_end_dt ' noline
    display $year_end_dt
  #endif

  !AXL 01/20/2002 - Begin
  Do format-datetime($year_end_dt,$year_end_dt_cmp,{DEFCMP},'','') 
  #ifdef debug8
     display '$Covg_Beg_Dt_cmp ' noline
     display $Covg_Beg_Dt_cmp
     display '$Covg_Elec_Dt_cmp ' noline
     display $Covg_Elec_Dt_cmp
  #endif
