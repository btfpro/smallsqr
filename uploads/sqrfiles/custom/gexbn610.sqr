begin-procedure P160-Print-Cover-Page
#debug9 Show 'P160-Print-Cover-Page'

 !Show 'P160-Print-Cover-Page'
  Print 'RUN CONTROL INFORMATION FOR THIS REPORT RUN:'            (+5,1)
  Print '$Prcs_OprID          ='                                  (+2,5)
