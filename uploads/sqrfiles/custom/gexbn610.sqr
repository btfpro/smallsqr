     If $Covg_Elec_Dt_cmp <> '19000101'
        let $exclude_flag='1'
     end-if
  end-if
  !AXL 01/20/2002 - End

  #ifdef debug8
     display '$exclude_flag ' noline
     display $exclude_flag
  #endif

FROM   PS_SAVINGS_PLAN     SP

WHERE  SP.EMPLID              = $EMPLID
AND    SP.EMPL_RCD           = #EMPL_RCD
AND    SP.PLAN_TYPE           = '40'

AND    SP.EFFDT               =
      (SELECT MAX(EFFDT)
       FROM   PS_SAVINGS_PLAN
       WHERE  EMPLID          = SP.EMPLID
       AND    EMPL_RCD       = SP.EMPL_RCD
       AND    PLAN_TYPE       = '40'
       AND    EFFDT          <= $year_end_dt)
End-Select
