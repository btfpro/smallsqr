!*********************************************************************
Begin-Procedure P200-Main-Process
!*********************************************************************
#debug9 Show 'P200-Main-Process'
    ! Show 'P200-Main-Process ***********************************************************'
Begin-Select
J.EMPLID
J.EMPL_RCD
J.COMPANY
J.ELIG_CONFIG1
PD.NAME
BNPER.HIGHLY_COMP_EMPL_P   !PD.HIGHLY_COMP_EMPL_P     ! ISDVNPK 08/25/2007 Modified for upgrade 9.0 added BN_PERSON
PD.BIRTHDATE
!AXL 04/13/2000 Begin
!E.UNION_CD
J.UNION_CD
!AXL 04/13/2000 End
E.HIRE_DT
!E.REHIRE_DT		!GEX-MXT COMMENTED FOR REHIRE DATE LOGIC IN 9.0
E.TERMINATION_DT
SYSDATE        &GEXJOB_REH_ASOFDATE 	!GEX-MXT ADDED FOR REHIRE DATE LOGIC IN 9.0

  Let $emplid       = &J.EMPLID
  Let #empl_rcd     = &J.EMPL_RCD
  Let $company      = &J.COMPANY
  Let $name         = &PD.NAME
  Let $config1      = Substr(&J.ELIG_CONFIG1,1,3)
  
  ! GEX-MXT 03/24/2008 Fixed for REHIRE DT-BEGIN
   let $GEXJOB_REH_EMPLID = $emplid
   let #GEXJOB_REH_EMPLRCD = #EMPL_RCD
!   let $GEXJOB_REH_ASOFDATE = &GEXJOB_REH_ASOFDATE
    Let $GEXJOB_REH_ASOFDATE = $year_end_dt                   ! ISDVASS used $year_end_dt instead of sysdate
   do Get-Job-Rehire-Dt
   if $GEXJOB_REH_REHIRE_DT <> ''
     let $GEXJOB_REH_REHIRE_DT = datetostr(strtodate($GEXJOB_REH_REHIRE_DT,'DD-MON-YYYY'),'DD-MON-YYYY') 
   end-if
  ! GEX-MXT 03/24/2008 Fixed for REHIRE DT-END 
  
  show '$emplid '   &J.EMPLID
  show '#empl_rcd ' &J.EMPL_RCD
  show '$company '  &J.COMPANY
  show '$name '     &PD.NAME
  show '$config1 '  &J.ELIG_CONFIG1
  show 'E.TERMINATION_DT ' &E.TERMINATION_DT 
  !show 'E.HIRE_DT ' &E.REHIRE_DT
  !show 'E.REHIRE_DT ' &E.REHIRE_DT
  show 'HIRE_DT ' $GEXJOB_REH_REHIRE_DT			! GEX-MXT 03/24/2008 Fixed for REHIRE DT
  show 'REHIRE_DT ' $GEXJOB_REH_REHIRE_DT		! GEX-MXT 03/24/2008 Fixed for REHIRE DT

  If &bnper.highly_comp_empl_p = 'H'   ! &pd.highly_comp_empl_p = 'H'
    Let $empl_type  = '2'
  Else
    Let $empl_type  = '0'
  End-If
!AXL 04/13/2000 Begin
  If &J.UNION_CD   = 'NON' or
     &J.UNION_CD   = 'SEC' or
     &J.UNION_CD   = 'CBT' or
     &J.UNION_CD   = 'COL' or	!AXL - 10/4/2000 - Add
     &J.UNION_CD   = 'TOL' or 	!AXL - 06/6/2001 - T.45.0755 Toledo Add
     &J.UNION_CD   = 'HGN' or	!AXL - 10/8/2001 - Add
     &J.UNION_CD   = 'MOD' or   !CSR - 10/14/2004 - Add
     &J.UNION_CD   = 'GGO'      !CXA - 10/20/2005 - Add

!AXL 04/13/2000 End
    Let $union_cd   = '0'
  Else
    Let $union_cd   = '1'
  End-If

  !AXL 10/8/2001 - Begin
  !Let $erncd_criteria = 'EB.ERNCD              IN (''BON'',''' || $ERNCD_SPCL || ''')'
  !Do P210-Earnings-Balance
  !AXL 10/8/2001 - End

  do Convert-To-DTU-Date(&PD.BIRTHDATE, $birthdate_dtu)
  do Convert-To-DTU-Date(&E.HIRE_DT, $hire_dt_dtu)
  do Convert-To-DTU-Date($year_end_dt, $year_end_dt_dtu)
  do dtu-diff-years($birthdate_dtu, $year_end_dt_dtu, #age)
  do dtu-diff-years($hire_dt_dtu, $year_end_dt_dtu, #service_years)

  !AXL 10/8/2001 - Begin
  Do P175-Max-Check-For-Partic-Row
  Do P190-Check-Against-Map
  If $Partic_Row_Flag='N'
     Do P180-Any-Partic-Row
     Do P190-Check-Against-Map
  End-if
 
  Let $erncd_criteria = 'EB.ERNCD              IN (''BON'',''' || $ERNCD_SPCL || ''')'
  Do P210-Earnings-Balance

  show '$Partic_Row_Flag ' $Partic_Row_Flag 

  If $Partic_Row_Flag='Y'
   #debug9 show 'Doing P250-Check-For-Enrollment'
     Do P250-Check-For-Enrollment

     ! DXS, 02/20/07. New Changes 
     ! get the balance for the new code.
     Let $erncd_criteria = 'NEW.ERNCD = ''' || $ERNCD_SPCL || ''''
     #debug9 show 'Doing New-Earnings-Balance'
     Do New-Earnings-Balance

     do Insert-Temp-Info
  End-if

  #ifdef debug8
    show '$year_end_run ' $year_end_run 
    show '$enrolled  ' $enrolled
    show '#dedcd-hours '   #dedcd-hours
    show '#service_years ' #service_years 
    show '#age '      #age      
    show '$config1 '      $config1
    show ' '
  #endif
 
  !AXL 10/8/2001 - Move next section to new procedure Begin
  !If $year_end_run = 'Y'
  !  If $enrolled = 'Y'
  !    Do P280-Exclude-Run-Control-Eligibles
  !   !Do P230-Write-Detail-Record
  !  End-If
  !Else
  !  If $enrolled = 'Y'
  !    Do P280-Exclude-Run-Control-Eligibles
  !    !Do P230-Write-Detail-Record
  !  Else
  !    If #dedcd-hours   >= 750   And
  !	 #service_years >= 1     And
  !	 #age           >= 21    And
  !	 $config1       <> 'SAV'
  !	Do P280-Exclude-Run-Control-Eligibles
  !    End-If
  !  End-If
  !End-If
  !AXL 10/8/2001 - Move next section to new procedure End
  !AXL 10/8/2001 - End

FROM   PS_PERSONAL_DATA    PD,
       PS_EMPLOYMENT       E,
       PS_JOB              J,
       PS_BN_PERSON        BNPER  ! ISDVNPK 08/25/2007 Modified for upgrade 9.0 added BN_PERSON
!AXL 10/8/2001 - Remove from main sel - Begin
      ! PS_BEN_PROG_PARTIC  BPP		

!WHERE  BPP.BENEFIT_PROGRAM     = $BENEFIT_PROGRAM

!AND    BPP.EFFDT               =
!      (SELECT MAX(EFFDT)
!       FROM   PS_BEN_PROG_PARTIC
!       WHERE  EMPLID           = BPP.EMPLID
!       AND    EMPL_RCD        = BPP.EMPL_RCD
!       AND    EFFDT           <= $year_end_dt)
!AND    J.EMPLID                = BPP.EMPLID
!AND    J.EMPL_RCD             = BPP.EMPL_RCD
!AXL 10/8/2001 - Remove from main sel - End

WHERE PD.EMPLID               = J.EMPLID 
AND   BNPER.EMPLID = PD.EMPLID     ! ISDVNPK 08/25/2007 Modified for upgrade 9.0 added BN_PERSON  
AND    E.EMPLID                = J.EMPLID
AND    E.EMPL_RCD             = J.EMPL_RCD

AND    J.EFFDT                 =
      (SELECT MAX(EFFDT)
       FROM   PS_JOB
       WHERE  EMPLID           = J.EMPLID
       AND    EMPL_RCD        = J.EMPL_RCD
       AND    EFFDT           <= $year_end_dt)

AND    J.EFFSEQ                =
      (SELECT MAX(EFFSEQ)
       FROM   PS_JOB
       WHERE  EMPLID           = J.EMPLID
       AND    EMPL_RCD        = J.EMPL_RCD
       AND    EFFDT            = J.EFFDT)

!AXL 10/8/2001 - new term code exclusion - Begin
AND    (E.TERMINATION_DT IS NULL OR
       (E.TERMINATION_DT between $year_begin_dt and $year_end_dt) )
!AXL 10/8/2001 - new term code exclusion - End

AND J.COMPANY > '399' 		!AXL 10/8/2001 Exclude IND
! and j.emplid = '176626259'	! DXS, testing
ORDER BY J.EMPLID

End-Select
     !Show 'P200-Main-Process #################################################################'
End-Procedure
!***********************************************************************
!*********************************************************************
Begin-Procedure P175-Max-Check-For-Partic-Row
!*********************************************************************
!Determine the max row
#debug9 Show 'P175-Max-Check-For-Partic-Row'
     
   #ifdef debug8
      show 'P175-Max-Check-For-Partic-Row '
      show '$BENEFIT_PROGRAM ' $BENEFIT_PROGRAM
      show '$EMPLID '  $EMPLID 
      show '#EMPL_RCD ' #EMPL_RCD
   #endif

   Let $BENEFIT_PROGRAM = ' '
	
Begin-Select
BPP.BENEFIT_PROGRAM
        Let $BENEFIT_PROGRAM = &BPP.BENEFIT_PROGRAM

FROM PS_BEN_PROG_PARTIC  BPP

WHERE  BPP.EMPLID              = $EMPLID
AND    BPP.EMPL_RCD           = #EMPL_RCD

AND    BPP.EFFDT               =
      (SELECT MAX(EFFDT)
       FROM   PS_BEN_PROG_PARTIC
       WHERE  EMPLID           = BPP.EMPLID
       AND    EMPL_RCD        = BPP.EMPL_RCD
       AND    EFFDT            < $year_begin_dt)

End-Select
	#debug8 show '$BENEFIT_PROGRAM ' $BENEFIT_PROGRAM

End-Procedure
!***********************************************************************
!*********************************************************************
Begin-Procedure P180-Any-Partic-Row
!*********************************************************************
!Find any qualifying row
#debug9 Show 'P180-Any-Partic-Row'
   
   Let $BENEFIT_PROGRAM = ' '
	
Begin-Select
BPPX.BENEFIT_PROGRAM
	
        Let $BENEFIT_PROGRAM = &BPPX.BENEFIT_PROGRAM

FROM PS_BEN_PROG_PARTIC  BPPX

WHERE  BPPX.EMPLID              = $EMPLID
AND    BPPX.EMPL_RCD           = #EMPL_RCD

AND    BPPX.EFFDT between $year_begin_dt and  $year_end_dt

End-Select
!***********************************************************************
End-Procedure
