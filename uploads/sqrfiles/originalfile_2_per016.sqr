            IF #EEOcol = 7
                DO Format-Number(#old_col7, $out, '9999999')
                ARRAY-ADD #old_col7 to EEOarray(#EEOrow) count(#EEOcol)
             END-IF
             IF #EEOcol = 8
                DO Format-Number(#old_col8, $out, '9999999')
                ARRAY-ADD #old_col8 to EEOarray(#EEOrow) count(#EEOcol)
             END-IF
             IF #EEOcol = 9
                DO Format-Number(#old_col9, $out, '9999999')
                ARRAY-ADD #old_col9 to EEOarray(#EEOrow) count(#EEOcol)
             END-IF
             IF #EEOcol = 10
                DO Format-Number(#old_col10, $out, '9999999')
                ARRAY-ADD #old_col10 to EEOarray(#EEOrow) count(#EEOcol)

             END-IF

             IF $SECTIONBC = 'Y'
                ! if this establishment is in Hawaii
                ! print out only the values of total and categories (B) and (C)
                !---------------------------------------------------------------

                IF $Hawaii_Flag = 'Y' AND $Consolidated <> 'Y'
                   IF #EEOcol = 0 OR #EEOcol = 1 OR #EEOcol = 6
                      PRINT $out (#line,#col,#len)
                   END-IF
                ELSE
                   PRINT $out (#line,#col,#len)
                END-IF
             END-IF

             ADD 1 TO #EEOcol
         END-WHILE

         IF $ReportType = 'GOVT'
            DO Insert-Previous-Totals        !Insert your current totals into the previous total tbl.
         END-IF
      END-IF

      ADD 1 to #EEOrow

   END-WHILE

   NEW-PAGE
END-IF


End-Procedure Print-Totals

