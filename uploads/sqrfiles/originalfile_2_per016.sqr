! resets EEOarray.
!----------------------------------------------------------------------
Begin-Procedure Check-Estab-Total
#IFDEF DEBUGPER016
  DISPLAY 'FLOW: Check-Estab-Total'
#ENDIF

GET #est_tot FROM EEOarray(10) count(0)

#IFDEF DEBUGPER016
  DISPLAY '   #est_tot: ' NOLINE
