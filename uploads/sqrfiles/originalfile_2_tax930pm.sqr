!***********************************************************************
begin-procedure Open-File
!***********************************************************************
  move 0 to #FileCount
  if #FileExtension <> 0
    close 10
  end-if
  #ifdef OS400
     let $FileID = '{IMPORTPREFIX}' || 'IRSTAX'
  #else
     let $FileID = 'IRSTAX'
     let $FileID = '{IMPORTPREFIX}' || $FileID || '{IMPORTSUFFIX}'
