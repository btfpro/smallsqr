if $EE_Selected <> 'Y'
   display ''
   display 'No employees selected. Program stopped.'
   display ''
   stop
end-if

let $SelectedEEs = SUBSTR($SelectedEEs,1,LENGTH($SelectedEEs) - 2)
let $SelectedEEs = $SelectedEEs || ')'

end-procedure

