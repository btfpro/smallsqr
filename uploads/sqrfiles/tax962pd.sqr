begin-procedure Print-File-Totals
  print 'File Totals - Wages: ' (+2,1)
  print  #F_StateWages (0,22) edit 999,999,999.99   !State Taxable Wages
  print 'Taxes: ' (+1,15)
  print  #F_StateTax   (0,22) edit 999,999,999.99   !State Taxes
