end-procedure

begin-procedure Process-RE-Record
  if $ErName <> ''
    if $TotalsOnly = 'N'
      new-page
      move ' ' to $Col1Hdr1
      move ' ' to $Col2Hdr1
      move ' ' to $Col3Hdr1
      move ' ' to $Col4Hdr1
      move ' ' to $Col5Hdr1
      move ' ' to $Col6Hdr1
      move ' ' to $Col1Hdr2
      move ' ' to $Col2Hdr2
      move ' ' to $Col3Hdr2
      move ' ' to $Col4Hdr2
      move ' ' to $Col5Hdr2
      move ' ' to $Col6Hdr2
    end-if
  end-if

  next-listing  NEED = 24
  do Print-RE-Record

  if $TotalsOnly = 'N'
    move $ErName              to $Employer
    new-page
    move 'Last'             to $Col1Hdr1
    move 'Name'             to $Col1Hdr2
    move 'First'            to $Col2Hdr1
    move 'Name'             to $Col2Hdr2
    move 'Middle'           to $Col3Hdr1
    move 'Name'             to $Col3Hdr2
    move 'SSN'              to $Col4Hdr2
    move '              '   to $Col5Hdr1
    move '   State Wages'   to $Col5Hdr2
    move '              '   to $Col6Hdr1
    move '   State Wages'   to $Col6Hdr2
  end-if
end-procedure

