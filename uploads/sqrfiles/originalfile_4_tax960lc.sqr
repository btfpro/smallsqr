Begin-procedure Write-S-Record-from-Local

  if $Agency <> 'LJCKY'
    move &E.Emplid       to $E.Emplid
    move &E.Address1     to $E.Address2            !Most requirements are for street address to
    move &E.Address2     to $E.Address1            ! be in Address2
    move &E.City         to $E.City
    let  $E.StateCd = rtrim(&E.State, ' ')
    move &E.Postal       to $E.Zip
    move &E.Country      to $E.Country

    do Populate-EE-Address
  end-if

  Evaluate $Agency
    when = 'BTLCK'
    when = 'SAGIN'
    when = 'STLTA'
    when = 'LJCKY'
    when = 'WSTB'
    when = 'ALLEN'
    when = 'READ'
      if $FirstTime = 'Y'
        do Write-W-Record
        let $FirstTime = 'N'
      end-if
      do Write-S-Record
      move 0 to #Fed_Wages
      move 0 to #Fed_Tax
      break
    when = 'AKRON'
    when = 'CCA'
    when = 'CINCI'
    when = 'DAYTO'
    when = 'FINDL'
    when = 'JOHNS'
    when = 'MORAI'
    when = 'CYFLS'
    when = 'WSTRV'
    when = 'MASSI'
    when = 'RITA'
    when = 'CLMBS'
    when = 'SIDNE'
      do Ohio-Specific
      if $FirstTime = 'Y'
        do Write-W-Record
        let $FirstTime = 'N'
      end-if
      do Write-S-Record
      if $Agency <> 'CINCI'
         move 0 to #Fed_Wages
      end-if
      break
    when = 'WALKR'
    when = 'GRAPD'
    when = 'KCFD'
    when = 'RITA'
    when = 'CLMBS'
    when = 'LOUIS'
    when = 'AKRON'
    when = 'LSING'
    when = 'MRYSV'
    when = 'NWASH'
    when = 'WARRE'
      if $FirstTime = 'Y'
          do Write-W-Record
          do Write-O-Record
          let $FirstTime = 'N'
      end-if
      do Write-S-Record
      move 0 to #Fed_Wages
      move 0 to #Fed_Tax
      break
    when-other
      do Write-S-Record
      break
  end-evaluate
  if $Agency <> 'CINCI'
    move 0 to #State_Wages     !Only Report once
    move 0 to #State_Tax
  end-if
  move 0   to #Local_Wages
  move 0   to #Local_Tax
  move 0 to #State_EIC
end-procedure

