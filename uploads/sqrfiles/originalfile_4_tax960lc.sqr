Begin-Procedure Get-City-Name-RITA
  let $MoreCities = 'N'
  let $City-Code = rtrim(&LTT2.W2_MAG_CODE, ' ')
  evaluate $City-Code
    when = '6'
      let $City-Name = 'ASHVILLE'
      break
    when = '10'
      let $City-Name = 'AURORA'
      break
    when = '13'
      let $City-Name = 'ARLINGTON HTS'
      break
    when = '20'
      let $City-Name = 'AVON'
      break
    when = '21'
      let $City-Name = 'AVON LAKE'
      break
    when = '40'
      let $City-Name = 'BAY VILLAGE'
      break
    when = '50'
      let $City-Name = 'BEACHWOOD'
      break
    when = '51'
      let $City-Name = 'BEACHWOOD EAST JEDD'
      break
    when = '52'
      let $City-Name = 'BEACHWOOD WEST JEDD'
      break
    when = '65'
      let $City-Name = 'BEDFORD HTS'
      break
    when = '90'
      let $City-Name = 'BENTLEYVILLE'
      break
    when = '100'
      let $City-Name = 'BEREA'
      break
    when = '104'
      let $City-Name = 'BEXLEY'
      break
    when = '110'
      let $City-Name = 'BOSTON HTS'
