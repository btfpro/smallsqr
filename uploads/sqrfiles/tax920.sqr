  let $Local_Tax = TRANSLATE($Local_Tax,'Bb','99')              !Local tax
  move $Local_Tax to #Prv_Local_Tax                           ! previous
  move $Local_Tax to #Local_Tax                               ! current


  move 0 to #StateLocalCount

