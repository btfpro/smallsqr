  let #Alloc_Tips = 0                                      !Allocated tips
  let #Prv_Alloc_Tips = 99999999.99

  let #Adv_EIC = 99999999.99                             !EIC
  let #Prv_Adv_EIC = 0.00

  let #Dep_Care = 0                                       !Dep care
  let #Prv_Dep_Care = 99999999.99


