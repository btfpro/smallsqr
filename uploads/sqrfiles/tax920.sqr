begin-procedure Read-Employee-Data

  move 'N' to $EmployeeFound


begin-SELECT
E.COMPANY          () ON-BREAK LEVEL=1 PRINT=NEVER BEFORE=BEFORE-COMPANY
E.W2C_ADD_EMPLID   () ON-BREAK LEVEL=2 PRINT=NEVER BEFORE=BEFORE-EMPLID
E.W2C_SSN
E.W2C_NAME
E.ADDRESS1
E.ADDRESS2
E.CITY
E.STATE
E.POSTAL
E.COUNTRY
E.STATUTORY_EE
E.PENSION_PLAN
E.PRV_SSN
E.PRV_W2_NAME
E.PRV_ADDRESS1
E.PRV_ADDRESS2
E.PRV_CITY
E.PRV_STATE
E.PRV_POSTAL
E.PRV_COUNTRY
E.PRV_STATUTORY_EE
E.PRV_PENSION_PLAN
E.W2C_BATCH_NBR
E.CALENDAR_YEAR

  if $Slash_in_Name = 'Y'

    move 'Y' to $EmployeeFound
    move &E.CALENDAR_YEAR  to $ER_Year  9999

  do Process-W2C

    if &W2C.W2C_REG_REPRINT  = 'C'
      do Update-W2-History
    end-if
  end-if

FROM  PS_W2C_DATA E
  WHERE E.W2C_STATUS       <> 'V'
  [$E.Batch_Nbr]
  [$E.SelectedEEs]        !NULL string if SELECT EEs option not used
ORDER BY E.COMPANY, E.W2C_SSN
end-SELECT
end-procedure

begin-procedure Before-Company

begin-SELECT
A.W2_REPORTING_CO
A.DESCR
A.FEDERAL_EIN
A.ADDRESS1
A.ADDRESS2
A.CITY
A.STATE
A.POSTAL
A.COUNTRY
A.TAXFORM_ID

  move &A.Federal_EIN     to $Federal_EIN 099999999
  move $Federal_EIN       to $A.Federal_EIN xx-xxxxxxx
  move &A.Descr           to $A.Descr
  move &A.Address1        to $A.Address1
  move &A.Address2        to $A.Address2
  move &A.Country         to $A.Country
  let  $A.City            =  RTRIM(&A.City,' ')
  let  $A.State           =  RTRIM(&A.State,' ')
  move &A.POSTAL          to $A.Postal
  move &A.W2_Reporting_Co to $A.W2_Reporting_Co


  UPPERCASE $A.Descr
  UPPERCASE $A.Address1
  UPPERCASE $A.Address2
  UPPERCASE $A.Country
  UPPERCASE $A.City


FROM  PS_W2_COMPANY A
WHERE A.COMPANY  = &E.COMPANY
  AND A.W2_REPORTING_CO <> ' '
  AND A.TAXFORM_ID       = 'W'
  AND A.CALENDAR_YEAR = (SELECT MAX(CALENDAR_YEAR)
                         FROM PS_W2_COMPANY
                         WHERE A.COMPANY = COMPANY
                           AND A.TAXFORM_ID = TAXFORM_ID
                           AND CALENDAR_YEAR <= &E.CALENDAR_YEAR)
end-SELECT
end-procedure

begin-procedure Before-EmplID

  move &E.W2C_Name to $E.W2_Name
  find '/' in $E.W2_Name 0 #foundslash

  if #foundslash = -1
    let $Slash_in_Name = 'N'
    DISPLAY 'In the W-2c panel on the Maintain Payroll Data (US) - Use - '
    DISPLAY 'Prior 2001 W-2c Data menu selection, insert a slash (/) immediately before the last name'
    DISPLAY '(for example:  John J. /Smith).'
    DISPLAY $E.W2_Name
    DISPLAY 'EMPLID ' NOLINE
    DISPLAY &E.W2C_ADD_EMPLID
  else
    let $Slash_in_Name = 'Y'
    do Before-Emplid-Process
  end-if

end-procedure

begin-procedure Before-EmplID-Process

  move &E.W2C_ADD_EMPLID   to $E.EmplID

  move &E.W2C_SSN to $S123 xxx
  if $S123 = &Temp_SSN_Mask or RTRIM(&E.W2C_SSN, ' ') = ''
    move 'Applied For' to $E.SSN
  else
    move &E.W2C_SSN to $E.SSN xxx-xx-xxxx
  end-if

  move &E.PRV_SSN to $S123 xxx
  if $S123 = &Temp_SSN_Mask or RTRIM(&E.PRV_SSN, ' ') = ''
    move 'Applied For' to $E.Prv_SSN
  else
    move &E.PRV_SSN to $E.Prv_SSN xxx-xx-xxxx
  end-if


  move &E.Statutory_Ee       to $E.Statutory_Ee
  move &E.Pension_Plan       to $E.Pension_Plan

  move &E.Address1        to $E.Address1
  move &E.Address2        to $E.Address2
  move &E.Country         to $E.Country
  let  $E.City            =  RTRIM(&E.City,' ')
  let  $E.State           =  RTRIM(&E.State,' ')
  move &E.Postal          to $E.Postal

  move &E.Prv_W2_Name         to $E.Prv_W2_Name

  move &E.Prv_Address1        to $E.Prv_Address1
  move &E.Prv_Address2        to $E.Prv_Address2
  move &E.Prv_Country         to $E.Prv_Country
  let  $E.Prv_City            =  RTRIM(&E.Prv_City,' ')
  let  $E.Prv_State           =  RTRIM(&E.Prv_State,' ')
  move &E.Prv_Postal          to $E.Prv_Postal

  move &E.Prv_Statutory_Ee    to $E.Prv_Statutory_Ee
  move &E.Prv_Pension_Plan    to $E.Prv_Pension_Plan

  uppercase $E.W2_Name
  uppercase $E.Address1
  uppercase $E.Address2
  uppercase $E.Country
  uppercase $E.City

  uppercase $E.Prv_W2_Name
  uppercase $E.Prv_Address1
  uppercase $E.Prv_Address2
  uppercase $E.Prv_Country
  uppercase $E.Prv_City

  let $YE_W2C_FName  = SUBSTR($E.W2_Name,1,#foundslash)
  let $YE_W2C_LName  = SUBSTR($E.W2_Name,#foundslash +2,length($E.W2_Name))

  let  $YE_W2C_FName =  LTRIM($YE_W2C_FName,' ')
  let  $YE_W2C_LName =  LTRIM($YE_W2C_LName,' ')
  let  $YE_W2C_FName =  RTRIM($YE_W2C_FName,' ')
  let  $YE_W2C_LName =  RTRIM($YE_W2C_LName,' ')

  let  $Current_Name =  $YE_W2C_FName || $YE_W2C_LName

  find '/' in $E.Prv_W2_Name  0 #foundslash
  if #foundslash = -1
     let $Previous_Name =  $E.Prv_W2_Name
     let $Previous_Name_Without_Slash = $E.Prv_W2_Name
  else
     let $Previous_FName  = SUBSTR($E.Prv_W2_Name,1,#foundslash)
     let $Previous_LName  = SUBSTR($E.Prv_W2_Name,#foundslash +2,length($E.Prv_W2_Name))

     let $Previous_Name   = $Previous_FName || $Previous_LName
     let $Previous_Name_Without_Slash = $Previous_FName || ' ' || $Previous_LName
  end-if


  let $Continue-Processing = 'Y'
  while $Continue-Processing = 'Y'
     find ' ' in $Current_Name 0 #foundspace
     if #foundspace  <> -1
        let $Condensed_Name1 = SUBSTR($Current_Name,1,#foundspace)
        let $Condensed_Name2 = SUBSTR($Current_Name,#foundspace +2,length($Current_Name))
        let $Current_Name    = $Condensed_Name1 || $Condensed_Name2
     else
        let $Continue-Processing = 'N'
     end-if
  end-while

  let $Continue-Processing = 'Y'
  while $Continue-Processing = 'Y'
     find ' ' in $Previous_Name 0 #foundspace
     if #foundspace  <> -1
        let $Condensed_Prv_Name1 = SUBSTR($Previous_Name,1,#foundspace)
        let $Condensed_Prv_Name2 = SUBSTR($Previous_Name,#foundspace +2,length($Previous_Name))
        let $Previous_Name       = $Condensed_Prv_Name1 || $Condensed_Prv_Name2
        let $Previous_Name       = RTRIM($Previous_Name,' ')
     else
        let $Continue-Processing = 'N'
     end-if
  end-while

end-procedure

begin-procedure Process-W2C

  move 0 to #LocalPrintCount
  move 0 to #StateSWTCount
  move '' to $Prev_State
  move 0 to #StateLocalCount

  do Load-Amounts
  do Print-W2C

end-procedure

begin-procedure Print-W2C
  move 1 to #j

  while #j <= #FormsPerPage/#EmployeesPerPage  !loop for individual ee's
    do Next-Form
    do Print-ER-Data
    do Print-EE-Data
    do Print-Page1-Data
    do Print-Fed-Amounts
    if #StateLocalCount > 0
      move 0 to #NextStateLoc
      do Print-State-Local-Amounts
    else
      if $Print_Test_State_Local = 'Y'
        do Print-Test-State-Local-Amounts
      end-if
    end-if
    add 1 to #j
  end-while

  do Init-Values


end-procedure

