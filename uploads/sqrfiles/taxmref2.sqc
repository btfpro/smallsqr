  End-If

End-Procedure Validate-Required-Fields-RCE

Begin-Procedure Validate-Required-Fields-RCW

  let $Required_Field_Error = 'N'

  if rtrim($FirstName-C, ' ') = ''
     show ' '
     show $E.EmplID
     show 'ERROR: The Correct First Name cannot be blank.'
     let $Required_Field_Error = 'Y'
  end-if

  if rtrim($LastName-C, ' ') = ''
     show ' '
     show $E.EmplID
     show 'ERROR: The Correct Last Name cannot be blank.'
     let $Required_Field_Error = 'Y'
  end-if

  If $Required_Field_Error = 'Y'
     STOP
  End-If

End-Procedure Validate-Required-Fields-RCW
