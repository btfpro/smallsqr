!Begin RSI-HCM103478

Begin-Procedure Validate-Email

  Let $Valid_Email = 'Y'
  do Validate-Email-Address($RC960.EMAIL_ADDR,$Valid_Email)

  If $Valid_Email = 'N'
     show ' '
     show 'ERROR: The Contact Email Address is blank or does not meet the required format.'
     show 'Please review the Contact Email Address format to ensure compliance with the '
     show 'required Social Security Administration Email Address specifications. '
     show ' '
     STOP
  End-If
End-Procedure Validate-Email

!***********************************************************************
! Procedure Validate-Phone-Fax-Format
!***********************************************************************
Begin-Procedure Validate-Phone-Fax-Format

  let $Phone = rtrim(translate(&RC960.TELEPHONE_NBR,'0123456789',''), ' ')

  let $Fax = rtrim(translate(&RC960.CONTACT_FAX,'0123456789',''), ' ')

  if $Phone <> '' or $Fax <> ''
     if $Phone <> ''
        show ' '
        show 'ERROR: The Contact Phone Number does not meet the required format. '
        show 'Please validate the Employer Contact Telephone Number contains numeric values only. '
        show ' '
     end-if
     if $Fax <> ''
        show ' '
        show 'ERROR: Fax Number does not meet the required format. '
        show 'Please validate Fax Number contains numeric values only. '
        show ' '
     end-if
     STOP
  end-if

End-Procedure Validate-Phone-Fax-Format

