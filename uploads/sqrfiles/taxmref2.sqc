Begin-Procedure Validate-Required-Fields-RCA

  let $Required_Field_Error = 'N'

  if rtrim($FedEIN, ' ') = '000000000'
     show ' '
     show 'ERROR: The Submitter Employer Identification Number (Federal EIN) cannot be blank or zeroes.'
     let $Required_Field_Error = 'Y'
  end-if

  if rtrim($EmprPIN, ' ') = ''
     show ' '
     show 'ERROR: The User Identification (Employee PIN) cannot be blank.'
     let $Required_Field_Error = 'Y'
  end-if

  if rtrim($RC960.Contact_name_ssa, ' ') = ''
     show ' '
     show 'ERROR: The Contact Name cannot blank.'
     let $Required_Field_Error = 'Y'
  end-if

  if rtrim(&RC960.Telephone_Nbr, ' ') = ''
     show ' '
     show 'ERROR: The Contact Phone Number cannot blank.'
     let $Required_Field_Error = 'Y'
  end-if

  If $Required_Field_Error = 'Y'
     STOP
  End-If

End-Procedure Validate-Required-Fields-RCA

Begin-Procedure Validate-Required-Fields-RCE

  let $Required_Field_Error = 'N'

   if rtrim($A.Descr, ' ') = ''
     show ' '
     show 'ERROR: The Employer Name cannot be blank.'
     let $Required_Field_Error = 'Y'
  end-if

  If $Required_Field_Error = 'Y'
