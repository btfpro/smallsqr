print $GEMPLID                   (,31)
print $GCOMPANY                  (,46)
do Format-DateTime($Udate, $IEFFDT, {DEFMDY}, '', '')
print $IEFFDT                    (,57)
print $Nonestatus                (,75)
end-procedure


