FROM PS_FED_TAX_DATA B
WHERE B.COMPANY = $SCompany
  AND B.EFFDT = (SELECT MAX(EFFDT) FROM PS_FED_TAX_DATA B1
                  WHERE B1.EMPLID       = B.EMPLID
                    AND B1.COMPANY      = B.COMPANY
                    AND B1.EFFDT       <= $Duedt
                    AND B1.EFFDT       >= $JAN01DT)
  AND B.W5_PRCSS     = 'S'
  AND B.EIC_STATUS  <> 'N'
ORDER BY
B.COMPANY,
B.EMPLID,
B.EFFDT ASC
end-SELECT

do Emplid-Check-B
if $notify='Y'
   do Print-Rows
end-if

