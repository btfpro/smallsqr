Begin-Procedure Print-OF8-Page-One-Data

!GRAPHIC () FONT 5 10
ALTER-PRINTER POINT-SIZE=10 FONT=5

print  &a.position_nbr                                                   (5,114,0)

print ' '                                                                (+2,17,0)

if  &a.gvt_posn_loc       =   'H'
or  &a.gvt_posn_loc       =   'L'
    print 'X'                                                            (0,33,0)
end-if

if  &a.gvt_posn_loc       =   'F'
or  &a.gvt_posn_loc       =   'G'
    print 'X'                                                            (0,42,0)
end-if

print  $DeptLoc_Descr                                                    (0,51,0)
print  $PosLoc_Descr                                                     (0,83,0)
print  &b.gvt_opm_cert_nbr                                               (0,114,0)

if  &a.action_reason       =  'NEW'
    print 'X'                                                            (0,26,0)
else
    print ' '                                                            (0,26,0)
end-if

print  ' '                                                               (+2,29,0)

if  &b.flsa_status         =  'E'
    print 'X'                                                            (0,49,0)
end-if

if  &b.flsa_status         =  'N'  or   &b.flsa_status  = 'V'
    print 'X'                                                            (0,60,0)
end-if

if  &b.gvt_exec_fin_discl  =  'Y'
    print 'X'                                                            (0,70,0)
end-if

if  &b.gvt_emp_fin_int     =  'Y'
    print 'X'                                                            (0,97,0)
end-if

if  &b.gvt_ia_actions      =  'Y'
    print 'X'                                                            (0,112,0)
end-if

if  &b.gvt_ia_actions      =  'N'
    print 'X'                                                            (0,123,0)
end-if

print  ' '                                                               (+2,28,0)

if  &a.gvt_posn_occupied   =  '1'
    print  'X'                                                           (0,49,0)
end-if

if  &b.manager_level       =  '2'
    print  'X'                                                           (0,70,0)
end-if

if  &b.gvt_posn_sens_cd    =  '1'
    print  'X'                                                           (0,81,0)
end-if

if  &b.gvt_posn_sens_cd    =  '3'
    print  'X'                                                           (0,96,0)
end-if

print &a.gvt_comp_level                                                  (0,114,0)

print ' '                                                                (+1,28,0)

if  &a.gvt_posn_occupied   =  '2'
    print  'X'                                                           (0,49,0)
end-if

if  &b.manager_level       =  '2'
    print  'X'                                                           (0,70,0)
end-if

print ' '                                                                (+1,28,0)

if  &a.gvt_posn_occupied   =  '3'
    print  'X'                                                           (0,49,0)
end-if

if  &a.gvt_posn_occupied   =  '4'
    print  'X'                                                           (0,60,0)
end-if

if  &b.manager_level       <> '2'
    print  'X'                                                           (0,70,0)
end-if

if  &b.gvt_posn_sens_cd    =  '2'
    print  'X'                                                           (0,81,0)
end-if

if  &b.gvt_posn_sens_cd    =  '4'
    print  'X'                                                           (0,96,0)
end-if

do Format-DateTime(&a.effdt, $out, {DEFMDY},'','')
print  $out                                                              (0,114,0)

print  &b.gvt_official_descr                                             (+5,26,0)
print  &b.gvt_paren_title                                                (0,44,0)
print  &a.gvt_pay_plan                                                   (0,72,0)
print  &a.gvt_occ_series                                                 (0,84,0)
print  &a.grade                                                          (0,105,0)

print  &a.gvt_org_ttl_descr                                              (+9,12,0)
print  &b.gvt_paren_title                                                (0,30,0)
print  $name                                                             (0,71,0)

print  $company_descr                                                    (+2,12,0)
print  $dept_gvt_par_descr3                                              (0,71,0)

print  $sub_ag_descr                                                     (+2,12,0)
print  $dept_gvt_par_descr4                                              (0,71,0)

print  $dept_gvt_par_descr2                                              (+2,12,0)
print  $dept_gvt_par_descr5                                              (0,71,0)

if  rtrim (&b.gvt_posn_class_std, ' ')  <>  ''
    move &b.gvt_posn_class_std    to  $prntext
    move 41                       to  #PrnStartCol
    move 169                      to  #PrnStartRow
    move 55                       to  #MaxWrapWidth
    move 3                        to  #MoveLinesDown
    move 3                        to  #MaxPrnLines
    move 'N'                      to  $PrintBold
    do PrintWrap
end-if

