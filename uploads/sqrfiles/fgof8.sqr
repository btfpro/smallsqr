!***********************************************************************
!                                                                      *
!  Function:     Init-Report                                           *
!                                                                      *
!  Description:  Initializes data for the beginning of the report      *
!                processing.                                           *
!                                                                      *
!***********************************************************************
Begin-Procedure Init-Report

  move 'FGOF8'                            to $ReportID
  move 'Position Description Form (OF-8)' to $ReportTitle
  display $ReportTitle

  do Stdapi-Init

  if $prcs_process_instance = ''
    do Ask-Position-Descr
  else
    do Select-Parameters
    do Get-Position-Descr-Parameters
  end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Process-Main                                          *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Process-Main

let #count = 0
let $found_position_flag = 'N'
let $found_name_flag = 'N'
let $found_dept_flag = 'N'
let $found_comp_flag = 'N'
let $found_sub_flag = 'N'
let $found_loc_flag = 'N'
let $found_loc_pos_flag = 'N'

Begin-Select     LOOPS=1

A.POSITION_NBR
A.EFFDT
A.EFF_STATUS
A.DESCR
A.ACTION_REASON
A.DEPTID
A.JOBCODE
A.REPORTS_TO
A.LOCATION
A.COMPANY
A.GVT_SUB_AGENCY
A.GVT_PAY_PLAN
A.GRADE
A.GVT_COMP_LEVEL
A.GVT_ORG_TTL_DESCR
A.GVT_OCC_SERIES
A.GVT_PAREN_TITLE
A.GVT_POSN_LOC
A.GVT_POSN_OCCUPIED
A.GVT_POSN_SENS_CD
B.JOBCODE
B.EFFDT
B.EFF_STATUS
B.DESCR
B.MANAGER_LEVEL
B.FLSA_STATUS
B.GVT_OFFICIAL_DESCR
B.GVT_PAREN_TITLE
B.GVT_OPM_CERT_NBR
B.GVT_POSN_CLASS_STD
B.GVT_POSN_SENS_CD
B.GVT_IA_ACTIONS
B.GVT_EXEC_FIN_DISCL
B.GVT_EMP_FIN_INT

    move 'Y' to $found_position_flag

    do Get-Name
    do Get-Department-Data
    do Get-Agency-Data
    do Get-Sub-Agency-Data
    do Get-Location-Data
    do Get-Location-Data-Position

    do Print-OF8-Form

FROM  PS_POSITION_DATA A,
      PS_JOBCODE_TBL   B

WHERE A.POSITION_NBR = $Position and
 A.EFFDT = (SELECT MAX(I.EFFDT) from PS_POSITION_DATA I
                  where I.POSITION_NBR = A.POSITION_NBR
                    and I.EFFDT <= $AsOfDate)
  and A.EFF_STATUS = 'A'
  and B.JOBCODE = A.JOBCODE
  and B.EFFDT = (SELECT MAX(J.EFFDT) from PS_JOBCODE_TBL J
                  where J.JOBCODE = B.JOBCODE
                    and J.EFFDT <= A.EFFDT)
  and B.EFF_STATUS = 'A'

End-Select

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Name                                              *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Get-Name

Begin-Select

C.EMPLID
!*****************Begin Name Change Format*****************************
 ! C.NAME
C.NAME_DISPLAY
!*****************End   Name Change Format*****************************
D.EMPLID
D.EFFDT
D.EFFSEQ
D.POSITION_NBR

   move 'Y' to $found_name_flag

FROM  PS_PERSON_NAME   C,
      PS_JOB D
WHERE D.POSITION_NBR = &a.position_nbr
  and D.EFFDT = (SELECT MAX(K.EFFDT) from PS_JOB K
                  where K.EMPLID = D.EMPLID
                    and K.EFFDT >= &a.effdt
                    and K.EFFDT <= $AsOfDate)
  and C.EMPLID = D.EMPLID

End-Select

if $found_name_flag = 'N'
   let $Problem =  'No name associated with '||&a.position_nbr
   do Write-Report
   move 'Vacant' to $Name
   move ' ' to $Personal_Emplid
   move ' ' to $Job_Emplid
   move ''  to $Job_Effdt
   move 0   to #Job_Effseq
   move ' ' to $Job_Position_Nbr
else
!*********************************************Begin Name Change Format*****************************
   !move &c.name to $Name
   move &c.NAME_DISPLAY to $Name
!*********************************************End Name Change Format*****************************
   move &c.emplid to $Personal_Emplid
   move &d.emplid to $Job_Emplid
   move &d.effdt to $Job_Effdt
   move &d.effseq to #Job_Effseq
   move &d.position_nbr to $Job_Position_Nbr
end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Department-Data                                   *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
begin-procedure Get-Department-Data

Begin-Select

E.DEPTID
E.COMPANY
E.GVT_SUB_AGENCY
E.GVT_PAR_DESCR2
E.GVT_PAR_DESCR3
E.GVT_PAR_DESCR4
E.GVT_PAR_DESCR5
E.EFFDT
E.LOCATION

   move 'Y' to $found_dept_flag

FROM PS_DEPT_TBL E
WHERE
  E.DEPTID = &a.deptid
  and E.EFFDT = (SELECT MAX(L.EFFDT) from PS_DEPT_TBL L
                  where L.DEPTID = E.DEPTID
                    and L.EFFDT <= &a.effdt)

End-Select

if $found_dept_flag = 'N'
  let $Problem = 'Dept from PS_POSITION_DATA not in PS_DEPT_TBL'
  do Write-Report
  move ' ' to $Dept_Deptid
  move ' ' to $Dept_Company
  move ' ' to $Dept_Gvt_Sub_Agency
  move ' ' to $Dept_Gvt_Par_Descr2
  move ' ' to $Dept_Gvt_Par_Descr3
  move ' ' to $Dept_Gvt_Par_Descr4
  move ' ' to $Dept_Gvt_Par_Descr5
  move '' to $Dept_Effdt
  move ' ' to $Dept_Location
else
  move &e.deptid to $Dept_Deptid
  move &e.company to $Dept_Company
  move &e.gvt_sub_agency to $Dept_Gvt_Sub_Agency
  move &e.gvt_par_descr2 to $Dept_Gvt_Par_Descr2
  move &e.gvt_par_descr3 to $Dept_Gvt_Par_Descr3
  move &e.gvt_par_descr4 to $Dept_Gvt_Par_Descr4
  move &e.gvt_par_descr5 to $Dept_Gvt_Par_Descr5
  move &e.effdt to $Dept_Effdt
  move &e.location to $Dept_Location
end-if

if $found_dept_flag = 'Y'
   if $Dept_Gvt_Par_Descr2 = ' '
   move 'No Second Subdivision Found' to $Problem
   do Write-Report
   end-if
end-if

if $found_dept_flag = 'Y'
   if $Dept_Gvt_Par_Descr3 = ' '
      move 'No Third Subdivision Found' to $Problem
      do Write-Report
   end-if
end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Agency-Data                                       *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Get-Agency-Data

begin-SELECT
F.COMPANY
F.DESCR
F.EFFDT

   move 'Y' to $found_comp_flag

FROM  PS_COMPANY_TBL F
WHERE
  F.COMPANY = &e.company
  and F.EFFDT = (SELECT MAX(M.EFFDT) from PS_COMPANY_TBL M
                  where M.COMPANY = F.COMPANY
                    and M.EFFDT <= &a.effdt)

End-Select

if $found_comp_flag = 'N'
   let $Problem = 'Agency from PS_DEPT_TBL not in PS_COMPANY_TBL'
   Do Write-Report
   move ' ' to $Company_Company
   move ' ' to $Company_Descr
   move '' to $Company_Effdt
else
   move &f.company to $Company_Company
   move &f.descr to $Company_Descr
   move &f.effdt to $Company_Effdt
end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Sub-Agency-Data                                    *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Get-Sub-Agency-Data

Begin-Select

G.GVT_SUB_AGENCY
G.DESCR
G.EFFDT

  move 'Y' to $found_sub_flag

FROM  PS_GVT_SUBAGCY_TBL G
WHERE
  G.COMPANY = &e.company
  and G.GVT_SUB_AGENCY = &e.gvt_sub_agency
  and G.EFFDT = (SELECT MAX(N.EFFDT) from PS_GVT_SUBAGCY_TBL N
                  where N.COMPANY = G.COMPANY
                    and N.GVT_SUB_AGENCY = G.GVT_SUB_AGENCY
                    and N.EFFDT <= &a.effdt)

End-Select

if $found_sub_flag = 'N'
   let $Problem = 'Sub Agency from PS_DEPT_TBL not in PS_GVT_SUBAGCY_TBL'
   do Write-Report
   move ' ' to $Sub_Ag_Gvt_Sub_Agency
   move ' ' to $Sub_Ag_Descr
   move '' to $Sub_Ag_Effdt
else
   move &g.gvt_sub_agency to $Sub_Ag_Gvt_Sub_Agency
   move &g.descr to $Sub_Ag_Descr
   move &g.effdt to $Sub_Ag_Effdt
end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Location-Data                                     *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Get-Location-Data

Begin-Select

H.LOCATION
H.DESCR
H.EFFDT

   move 'Y' to $found_loc_flag

FROM  PS_LOCATION_TBL H
WHERE
  H.LOCATION = &e.location
  and H.EFFDT = (SELECT MAX(O.EFFDT) from PS_LOCATION_TBL O
                  where O.LOCATION = H.LOCATION
                    and O.EFFDT <= &a.effdt)

End-Select

if $found_loc_flag = 'N'
   Let $Problem = 'Location from PS_DEPT_TBL not in PS_LOCATION_TBL'
   do Write-Report
   move ' ' to $DeptLoc_Location
   move ' ' to $DeptLoc_Descr
   move '' to $DeptLoc_Effdt
else
   move &h.location to $DeptLoc_Location
   move &h.descr to $DeptLoc_Descr
   move &h.effdt to $DeptLoc_Effdt
end-if

End-Procedure


!***********************************************************************
!                                                                      *
!  Function:     Get-Location-Data-Position                            *
!                                                                      *
!  Description:                                                        *
!                                                                      *
!***********************************************************************
Begin-Procedure Get-Location-Data-Position

Begin-Select

I.LOCATION
I.DESCR
I.EFFDT

   move 'Y' to $found_loc_pos_flag

FROM  PS_LOCATION_TBL I
WHERE
  I.LOCATION = &a.location
  and I.EFFDT = (SELECT MAX(P.EFFDT) from PS_LOCATION_TBL P
                  where P.LOCATION = I.LOCATION
                    and P.EFFDT <= &a.effdt)

end-SELECT

if $found_loc_pos_flag = 'N'
   let $Problem = 'Location in PS_POSITION_DATA not it PS_LOCATION_TBL'
   do Write-Report
   move ' ' to $PosLoc_Location
   move ' ' to $PosLoc_Descr
   move '' to $PosLoc_Effdt
else
   move &i.location to $PosLoc_Location
   move &i.descr to $PosLoc_Descr
   move &i.effdt to $PosLoc_Effdt
end-if

End-Procedure


